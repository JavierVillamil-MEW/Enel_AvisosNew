{
  "name": "enel_avisos_theme",
  "version": "1.0.0",
  "private": true,
  "description": "Enel Avisos theme",
  "keywords": [
    "theme",
    "Enel"
  ],
  "license": "ISC",
  "author": "Fabian Perez",
  "main": "index.js",
  "scripts": {
    "livereload": "livereload --exclusions node_modules/ --exts 'css,js,twig,theme,apng,avif,gif,jpg,jpeg,jfif,pjpeg,pjp,png,svg,webp'",
