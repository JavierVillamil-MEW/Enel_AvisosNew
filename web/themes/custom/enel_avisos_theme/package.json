    "sass": "^1.97.0",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.1",
    "stylelint-webpack-plugin": "^5.0.1",
    "webpack": "^5.104.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "bootstrap": "^5.3.8"
